{
  "name": "natural-language-backend",
  "version": "0.0.1",
  "description": "Backend side for natural language project",
  "main": "src/app.js",
  "scripts": {
    "start": "npm run build && npm run serve",
    "startDev": "nodemon --watch src src/app.js --exec babel-node --presets es2015,stage-2",
    "build": "babel src -d dist",
    "serve": "node dist/app.js",
    "test": "mocha --compilers js:babel-register"
  },
  "author": "Gabriel Rodriguez and Mariano Furriel",
  "license": "ISC",
  "devDependencies": {
    "babel-register": "^6.11.6",
    "mocha": "^3.0.1",
    "nodemon": "^1.10.0"
  },
  "dependencies": {
    "babel-cli": "^6.11.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "body-parser": "^1.17.1",
    "express": "^4.15.2",
    "morgan": "^1.8.1",
    "prettyjson": "^1.2.1",
    "prose-js": "0.0.8",
    "swagger-tools": "^0.10.1",
    "swagger-ui": "^3.0.9",
    "yamljs": "^0.2.10"
  }
}